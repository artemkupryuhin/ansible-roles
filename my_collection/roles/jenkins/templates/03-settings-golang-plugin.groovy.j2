import org.jenkinsci.plugins.golang.GolangInstallation;
import hudson.tools.InstallSourceProperty;
import hudson.tools.ToolProperty;
import hudson.tools.ToolPropertyDescriptor;
import hudson.util.DescribableList;

def GoDesc = jenkins.model.Jenkins.instance.getExtensionList(org.jenkinsci.plugins.golang.GolangInstallation.DescriptorImpl.class)[0]

def isp = new InstallSourceProperty()
def autoInstaller = new org.jenkinsci.plugins.golang.GolangInstaller("1.14.2")
isp.installers.add(autoInstaller)

def proplist = new DescribableList<ToolProperty<?>, ToolPropertyDescriptor>()
proplist.add(isp)

def installation = new GolangInstallation("G1.14.2", "", proplist)

GoDesc.setInstallations(installation)
GoDesc.save()